{
  "name": "native-agent-monorepo",
  "private": true,
  "version": "0.0.1",
  "description": "A native driver to power agentic development for mobile applications",
  "keywords": [
    "agents",
    "mobile",
    "android",
    "ios"
  ],
  "type": "module",
  "types": "dist/index.d.ts",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist/",
    "README.md"
  ],
  "license": "MIT",
  "author": "Jeremy Cavallo",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "node --import tsx/esm src/index.ts"
  },
  "workspaces": ["packages/*", "apps/*"],
  "devDependencies": {
    "prettier": "^3.8.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0"
  },
  "overrides": {
    "unicorn-magic": "0.4.0"
  }
}
